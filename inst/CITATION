citHeader("To cite TMoE in publications use:")

citEntry(
  entry = "Article",
  title = "Robust mixture of experts modeling using the t-distribution",
  author = personList(person(given="F.", family="Chamroukhi")),
  journal = "Neural Networks - Elsevier",
  year = "2016",
  volume = "79",
  pages = "20--36",
  url = "https://chamroukhi.com/papers/TMoE.pdf",
  textVersion = "Chamroukhi F (2016). \u201cRobust mixture of experts modeling using the t-distribution.\u201d
  _Neural Networks - Elsevier_, *79*, pp. 20-36. <URL: https://chamroukhi.com/papers/TMoE.pdf>."
)

